@prefix@:
	mkdir -p @prefix@
	
BASETOOLS = @prefix@ make bison flex pkg-config sed tar tcl wget 

#gawk gzip grep coreutils findutils

allbasetools: ${BASETOOLS}

CVSTOOLS = ${BASETOOLS} cvs autoconf automake libtool swig

allcvstools: ${CVSTOOLS}

atk: ${ATK} ${BASETOOLS} glib
	@gzcat@ ${ATK} | tar xf -
	(cd `echo ${ATK} | sed s/.tar.gz//`; ./configure --prefix=@prefix@; make install; make clean)
	touch atk;
automake: ${AUTOMAKE} ${BASETOOLS}
autoconf: ${AUTOCONF} ${BASETOOLS}
	@gzcat@ ${AUTOCONF} | tar xf -
	(cd `echo ${AUTOCONF} | sed s/.tar.gz//`; ./configure --prefix=@prefix@; make install; make clean)
	touch autoconf;
